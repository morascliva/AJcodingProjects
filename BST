#include<stdio.h>
#include<stdlib.h>
struct node
{   int info;
    struct node*llink;
    struct node*rlink;
};
typedef structure*NODE;
NODE getnode()
{  NODE x;
   x=(NODE)malloc(sizeof(struct node));
   return x;
}
NODE Insert(int item,NODE root)
{    NODE,temp,cur,pre;
     int i;
     temp=getnode();
     temp->info=item;
     temp->llink=temp->rlink=NULL;
     if(root == NULL)
       {  root = temp;
          return root;
       }
       else
       prev = null;
       cur = root;
       while(cur!=NULL)
            {  prev =cur;
               cur=(temp->info<cur->info)?cur->llink:cur->rlink;
            }
        if (temp->info<prev->info)
        prev ->llink=temp;
        elseif (temp ->infoprev->info)
        prev->rlink=temp;
        return root;
        }
 }
