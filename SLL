      NODE Insertionend(NODE FIRST)
      { NODE last =FIRST;
        printf ("Enter the details of the new student \n ");
        read();
        if(last!=NULL)
        {   while(last->link!=NULL)
             {      last = last ->link;
             }
             last ->link=temp;
             }
             else
             {
                FIRST = temp;
             }
             return FIRST ;
             }
